{
  "name": "tesseract-server",
  "version": "0.0.0",
  "main": "server/server.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm:clean\" \"npm:build\" \"npm:start-server\"",
    "start-server": "node dist/server/index.js",
    "build": "npm run clean && npm run build-ng && npm run build-server",
    "build-ng": "ng build --prod",
    "build-server": "babel ./server --out-dir dist/server/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "watch": "concurrently \"npm:clean\" \"npm:watch-ng\" \"npm:watch-server\"",
    "watch-ng": "ng build --watch",
    "watch-server": "nodemon --exec babel-node ./server/server.js",
    "clean": "rimraf dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/pwa": "^0.8.3",
    "@angular/router": "^6.1.9",
    "@angular/service-worker": "^6.1.9",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "mongoose": "^5.2.17",
    "morgan": "^1.9.1",
    "rxjs": "~6.3.3",
    "shelljs": "^0.8.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.3",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "codelyzer": "~4.3.0",
    "concurrently": "^4.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.4",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
